{
  "name": "rnboilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "bundle-ios": "react-native bundle --dev false --entry-file index.js --bundle-output ios/main.jsbundle --platform ios",
    "bundle-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "clean-android-assets": "rm -rf android/app/src/main/res/drawable-* && rm -rf android/app/src/main/res/raw",
    "ts": "tsc --watch --noEmit --skipLibCheck",
    "lint": "eslint . --ext .ts --ext .tsx --max-warnings 0 --fix",
    "eslint-fix-all": "eslint --fix src/**/*.{js,ts,tsx}",
    "prettier-fix-all": "prettier --write src/**/*.{js,ts,tsx,json}",
    "eslint-check-all": "eslint src/**/*.{js,ts,tsx}",
    "prettier-check-all": "prettier --check src/**/*.{js,ts,tsx,json}",
    "clean-modules": "rm -rf node_modules/ && yarn cache clean",
    "pod-clear": "rm -rf ~/Library/Caches/CocoaPods && rm -rf Pods && rm -rf ~/Library/Developer/Xcode/DerivedData/* && cd ios && pod deintegrate && pod setup",
    "prepare-husky": "husky install",
    "gitflow:husky": "git config gitflow.path.hooks .husky",
    "chmod:husky": "chmod +x .husky/pre-commit",
    "link": "react-native link",
    "pull": "amplify pull",
    "compile": "amplify api gql-compile",
    "codegen": "amplify codegen",
    "graph-generate-codegen": "graphql-codegen --config codegen.yml",
    "graph-codegen": "yarn graphql-codegen --config codegen.yml",
    "graph": "yarn pull && yarn compile && yarn codegen && yarn graph-codegen",
    "prepare-env-ios": "yarn install && rm -rf ios/Pods && rm -rf ios/Podfile.lock && cd ios && pod deintegrate && pod install && cd ..",
    "prepare-env-android": "cd android && ./gradlew clean && ./gradlew :app:bundleRelease && yarn install",
    "pod-clear-m1": "rm -rf ~/Library/Caches/CocoaPods && rm -rf Pods && rm -rf ~/Library/Developer/Xcode/DerivedData/* && cd ios && arch -x86_64 pod deintegrate && arch -x86_64 pod setup",
    "pod-install-m1": "cd ios && arch -x86_64 pod install && cd ..",
    "clean-modules-m1": "rm -rf node_modules/ && arch -x86_64 yarn cache clean",
    "install-m1": "arch -x86_64 yarn install"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint . --ext .ts --ext .tsx --max-warnings 0"
    ],
    "*.{json}": [
      "prettier --write"
    ]
  },
